{
  "name": "dotoriham-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --run --reporter verbose --globals",
    "test:ui": "vitest --ui",
